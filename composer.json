{
    "name": "marifetler/composer-cleanup-forproduction-plugin",
    "type": "composer-cleanup-forproduction-plugin",
    "description": "A composer cleanup plugin, to remove tests and documentation to save space",
    "license": "MIT",
    "authors": [
        {
            "name": "Mecal Topantas",
            "email": "mecal@gmail.com"
        }
    ],
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Marifetler\\Composer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "class": "Marifetler\\Composer\\CleanupPlugin"
    }
}
